<h1>Log in</h1>

<%= form_for(@user, :url=>attempt_login_users_path(), :html=> {:method=>'get'}) do |f| %>
  <div class="field">
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </div>
  <div class="actions">
    <%= f.submit 'Login in' %>
  </div>
<% end %>
